@article{biais_gerard_orhago_2003,
	title = {{ORHAGO}},
	doi = {10.18142/23},
	abstract = {Les campagnes ORHAGO (Observation des Ressources HAlieutiques benthiques du GOlfe de Gascogne) s'inscrivent dans la mission d'observation et d'expertise en appui à la gestion des pêches de l'Ifremer. Elles visent principalement l'obtention de séries indices d'abondance pour les poissons plats et en particulier pour la sole. En conséquence, le choix a été fait d'adopter un chalut à perche pour se conformer aux normes du Conseil International pour l'Exploration de la Mer (CIEM) pour les poissons plats. Ce choix a permis de faire partie des campagnes coordonnées par le CIEM et ainsi d'être membre d'une communauté au sein de laquelle la méthodologie et les résultats peuvent être discutés chaque année lors des groupes de travail WGBEAM du CIEM.Depuis 2013, les campagnes ORHAGO sont utilisées pour l'évaluation de l'état du stock de sole du golfe de Gascogne. Elles permettent que cette évaluation soit analytique, c'est-à-dire effectuée avec un modèle permettant d'analyser et simuler la dynamique du stock. La sole est ainsi, avec le bar, une des deux seules ressources halieutiques internes au golfe de Gascogne pour laquelle ce niveau d'analyse peut être atteint et largement grâce à la série ORHAGO qui a un poids majeur dans l'évaluation.Outre la sole, les campagnes ORHAGO fournissent aussi des indices d'abondance pour l'ensemble des espèces pêchées, y compris le benthos non commercial. Elles sont ainsi une source d'informations sur l'évolution des peuplements et des habitats benthiques côtiers du golfe de Gascogne.Les campagnes ORHAGO permettent ainsi de : garantir la qualité de l'estimation de la mortalité par pêche subie par la sole, espèce économiquement majeure du golfe de Gascogne, disposer d'informations sur le recrutement de cette espèce pour améliorer les prévisions à court terme de l'effet de la pêche, avoir une série d'indices permettant de surveiller l'évolution des écosystèmes benthiques exploités par la pêche.},
	author = {BIAIS Gérard},
	year = {2003},
	note = {Publisher: Sismer},
}

