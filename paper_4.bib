	note = {Publisher: Royal Society},
	pages = {20140698},
	file = {Full Text PDF:C\:\\Users\\balglave\\Zotero\\storage\\EDU4GG6F\\Gimenez et al. - 2014 - Statistical ecology comes of age.pdf:application/pdf;Snapshot:C\:\\Users\\balglave\\Zotero\\storage\\RMJJZMSB\\rsbl.2014.html:text/html},
